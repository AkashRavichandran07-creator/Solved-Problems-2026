#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

  char num[1000];
  int count[1000] = {0},i;
  scanf("%s",num);
  for( i=0;num[i]!='\0';i++){
    if((num[i]>='0')&&(num[i]<='9')){
        count[num[i]-'0']++;
    }
  } for(i=0;i<10;i++){
    printf("%d ",count[i]);
  }               
    return 0;
}
